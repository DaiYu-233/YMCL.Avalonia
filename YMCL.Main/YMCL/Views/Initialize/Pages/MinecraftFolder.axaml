            <DockPanel>
                <StackPanel Margin="0,10,25,80" VerticalAlignment="Center">
                    <TextBlock DockPanel.Dock="Top" FontFamily="{DynamicResource Font}">
                        <Run Text="{Binding Source={x:Static properties:LangHelper.Current}, Path=Resources.ExistingNow}" />
                        <Run Text=":" />
                    </TextBlock>
                    <ListBox
                        DockPanel.Dock="Top"
                        Margin="0,10,0,0"
                        MaxHeight="320"
                        Name="MinecraftFolderListBox"
                        SelectedIndex="0"
                        VerticalAlignment="Top">
                        <ListBox.ItemTemplate>
                            <DataTemplate DataType="data:MinecraftFolder">
                                <TextBlock
                                    FontFamily="{DynamicResource Font}"
                                    Margin="0,5"
                                    TextWrapping="Wrap">
                                    <Run>[</Run>
                                    <Run Text="{Binding Name}" />
                                    <Run>]</Run>
                                    <Run Text="{Binding Path}" />
                                </TextBlock>
                            </DataTemplate>
                        </ListBox.ItemTemplate>
                    </ListBox>
