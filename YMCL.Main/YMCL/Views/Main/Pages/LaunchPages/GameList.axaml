<UserControl
    d:DesignHeight="450"
    d:DesignWidth="800"
    mc:Ignorable="d"
    x:Class="YMCL.Views.Main.Pages.LaunchPages.GameList"
    x:DataType="da:Data"
    xmlns="https://github.com/avaloniaui"
    xmlns:classes="clr-namespace:YMCL.Public.Classes"
    xmlns:converter="clr-namespace:YMCL.Public.Module.Ui.Converter"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:da="clr-namespace:YMCL.Public.Const"
    xmlns:data="clr-namespace:YMCL.Public.Classes.Data"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:operate="clr-namespace:YMCL.Public.Classes.Operate"
    xmlns:properties="clr-namespace:YMCL.Public.Langs"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Border
        Background="{DynamicResource 1x}"
        ClipToBounds="True"
        CornerRadius="{DynamicResource MainCornerRadius}"
        Name="VersionListRoot"
        Padding="10">
        <DockPanel>
            <DockPanel DockPanel.Dock="Top" VerticalAlignment="Top">
                <Button
                    Content="{Binding Source={x:Static properties:LangHelper.Current}, Path=Resources.ReturnWithIcon}"
                    FontFamily="{DynamicResource Font}"
                    Margin="0,0,5,0"
                    Name="CloseBtn" />
                <Button
                    Content="{Binding Source={x:Static properties:LangHelper.Current}, Path=Resources.OpenFolder}"
                    FontFamily="{DynamicResource Font}"
                    Margin="0,0,5,0"
                    Name="OpenSelectedMinecraftFolderBtn" />
                <Button
                    Content="{Binding Source={x:Static properties:LangHelper.Current}, Path=Resources.Refresh}"
                    FontFamily="{DynamicResource Font}"
                    Margin="0,0,5,0"
                    Name="RefreshListBtn" />
                <ComboBox
                    FontFamily="{DynamicResource Font}"
                    HorizontalAlignment="Stretch"
                    ItemsSource="{Binding MinecraftFolders}"
                    Name="MinecraftFolderComboBox"
                    SelectedItem="{Binding SettingEntry.MinecraftFolder}">
                    <ComboBox.ItemTemplate>
                        <DataTemplate DataType="data:MinecraftFolder">
                            <TextBlock
                                FontFamily="{DynamicResource Font}"
                                Margin="0,5"
                                TextWrapping="Wrap">
                                <Run>[</Run>
                                <Run Text="{Binding Name}" />
                                <Run>]</Run>
                                <Run Text="{Binding Path}" />
                            </TextBlock>
                        </DataTemplate>
                    </ComboBox.ItemTemplate>
                </ComboBox>
            </DockPanel>
            <ListBox
                FontFamily="{DynamicResource Font}"
                ItemsSource="{Binding CurrentFolderGames}"
                Margin="0,5,0,0"
                Name="GameListView"
                SelectedItem="{Binding UiProperty.SelectedMinecraft}">
                <ListBox.Styles>
                    <Style Selector="ListBoxItem">
                        <Setter Property="Margin" Value="5,0" />
                    </Style>
                </ListBox.Styles>
                <ListBox.ItemTemplate>
                    <DataTemplate DataType="operate:MinecraftDataEntry">
                        <DockPanel>
                            <StackPanel Orientation="Horizontal">
                                <Border
                                    ClipToBounds="True"
                                    CornerRadius="{DynamicResource MainCornerRadius}"
                                    DockPanel.Dock="Left"
                                    Height="38"
                                    Margin="0,5"
                                    Width="38">
                                    <Image Source="{Binding Icon}" />
                                </Border>
                                <StackPanel
                                    DockPanel.Dock="Right"
                                    Margin="10,0,0,0"
                                    VerticalAlignment="Center">
                                    <TextBlock FontFamily="{DynamicResource Font}" Text="{Binding Id}" />
                                    <TextBlock
                                        FontFamily="{DynamicResource Font}"
                                        FontSize="12"
                                        Foreground="{DynamicResource GrayColor}"
                                        Margin="1,0,0,0">
                                        <Run Text="{Binding ListTip}" />
                                        <Run Text="{Binding Loaders}" />
                                    </TextBlock>
                                </StackPanel>
                            </StackPanel>
                            <StackPanel
                                DockPanel.Dock="Right"
                                HorizontalAlignment="Right"
                                Orientation="Horizontal"
                                Spacing="15">
                                <StackPanel.Styles>
                                    <Style Selector="StackPanel">
                                        <Setter Property="Opacity" Value="0" />
                                        <Setter Property="Transitions">
                                            <Transitions>
                                                <DoubleTransition Duration="0:0:0.1" Property="Opacity" />
                                            </Transitions>
                                        </Setter>
                                    </Style>
                                </StackPanel.Styles>
                                <StackPanel.IsVisible>
                                    <Binding Path="IsPointerOver" RelativeSource="{RelativeSource AncestorType=ListBoxItem}" />
                                </StackPanel.IsVisible>
                                <StackPanel.Opacity>
                                    <Binding Path="IsPointerOver" RelativeSource="{RelativeSource AncestorType=ListBoxItem}">
                                        <Binding.Converter>
                                            <converter:BoolToDoubleConverter FalseValue="0.5" TrueValue="1" />
                                        </Binding.Converter>
                                    </Binding>
                                </StackPanel.Opacity>
                                <Button
