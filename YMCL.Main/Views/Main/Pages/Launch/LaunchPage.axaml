                                            <Run Text=" " />
                                            <Run Text="{x:Static properties:MainLang.Overview}" />
                                            <Run Text="         " />
                                        </TextBlock>
                                    </DockPanel>
                                </ui:TabViewItem.Header>
                                <Border
                                    Background="{DynamicResource 2x}"
                                    ClipToBounds="True"
                                    CornerRadius="5"
                                    HorizontalAlignment="Stretch"
                                    VerticalAlignment="Stretch">
                                    <StackPanel Margin="10" Spacing="10">
                                        <Border
                                            Background="{DynamicResource 1x}"
                                            ClipToBounds="True"
                                            CornerRadius="5"
                                            Padding="10">
                                            <StackPanel Margin="2,0,0,0">
                                                <TextBlock
                                                    FontFamily="{DynamicResource Font}"
                                                    Name="OverviewVersionId"
                                                    Text="1.12.2" />
                                                <TextBlock
                                                    FontFamily="{DynamicResource Font}"
                                                    FontSize="12"
                                                    Foreground="{DynamicResource GrayColor}"
                                                    Name="OverviewVersionInfo"
                                                    Text="Fabric 1.12.2 Java9" />
                                            </StackPanel>
                                        </Border>
                                        <Border
                                            Background="{DynamicResource 1x}"
                                            ClipToBounds="True"
                                            CornerRadius="5"
                                            Padding="10">
                                            <WrapPanel>
                                                <Button
                                                    Click="OpenVersionFolder"
                                                    Content="{x:Static properties:MainLang.VersionFolder}"
                                                    FontFamily="{DynamicResource Font}"
                                                    FontSize="13"
                                                    Margin="0,0,5,0"
                                                    Tag="" />
                                                <Button
                                                    Click="OpenVersionFolder"
                                                    Content="{x:Static properties:MainLang.SavesFolder}"
                                                    FontFamily="{DynamicResource Font}"
                                                    FontSize="13"
                                                    Margin="5,0,5,0"
                                                    Tag="saves" />
                                                <Button
                                                    Click="OpenVersionFolder"
                                                    Content="{x:Static properties:MainLang.ModsFolder}"
                                                    FontFamily="{DynamicResource Font}"
                                                    FontSize="13"
                                                    Margin="5,0,5,0"
                                                    Tag="mods" />
                                                <Button
                                                    Click="OpenVersionFolder"
                                                    Content="{x:Static properties:MainLang.ResourcepacksFolder}"
                                                    FontFamily="{DynamicResource Font}"
                                                    FontSize="13"
                                                    Margin="5,0,5,0"
                                                    Tag="resourcepacks" />
                                                <Button
                                                    Click="OpenVersionFolder"
                                                    Content="{x:Static properties:MainLang.ShaderpacksFolder}"
                                                    FontFamily="{DynamicResource Font}"
                                                    FontSize="13"
                                                    Margin="5,0,5,0"
                                                    Tag="shaderpacks" />
                                            </WrapPanel>
                                        </Border>
                                    </StackPanel>
                                </Border>
