                                                    Content="{x:Static properties:MainLang.DisableSelect}"
                                                    FontFamily="{DynamicResource Font}"
                                                    Margin="5,0,0,0"
                                                    Name="DisableSelectModBtn" />
                                                <Button
                                                    Content="{x:Static properties:MainLang.DeleteSelect}"
                                                    FontFamily="{DynamicResource Font}"
                                                    Margin="5,0,0,0"
                                                    Name="DeleteSelectModBtn" />
                                                <Button
                                                    Content="{x:Static properties:MainLang.Refresh}"
                                                    FontFamily="{DynamicResource Font}"
                                                    Margin="5,0,0,0"
                                                    Name="RefreshModBtn" />
                                                <TextBox
                                                    FontFamily="{DynamicResource Font}"
                                                    HorizontalAlignment="Stretch"
                                                    Margin="5,0,0,0"
                                                    Name="ModSearchBox"
                                                    Watermark="{x:Static properties:MainLang.SearchKeyword}" />
                                            </DockPanel>
                                            <ListBox
                                                DockPanel.Dock="Bottom"
                                                Margin="5"
                                                Name="ModManageList"
                                                SelectionMode="Multiple,Toggle">
                                                <ListBox.ItemTemplate>
                                                    <DataTemplate DataType="cl:ModManageEntry">
                                                        <TextBlock
                                                            FontFamily="{DynamicResource Font}"
                                                            FontWeight="{DynamicResource FontWeight}"
                                                            Foreground="{DynamicResource TextColor}"
                                                            Text="{Binding Name}"
                                                            TextDecorations="{Binding Decorations}"
                                                            VerticalAlignment="Center" />
                                                    </DataTemplate>
                                                </ListBox.ItemTemplate>
                                            </ListBox>
                                        </DockPanel>
                                        <TextBlock
                                            FontFamily="{DynamicResource Font}"
                                            Foreground="{DynamicResource GrayColor}"
                                            HorizontalAlignment="Right"
                                            Margin="0,40,10,0"
                                            Name="SelectedModCount" />
                                    </Grid>
                                </Border>
                            </ui:TabViewItem>
                        </ui:TabView>
                    </Border>
                </DockPanel>
            </Border>
        </Grid>
    </Border>
</UserControl>