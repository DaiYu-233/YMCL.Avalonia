<UserControl
    x:Class="YMCL.Main.Views.Main.Pages.Download.Pages.AutoInstall.AutoInstallPage"
    xmlns="https://github.com/avaloniaui"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:ml="clr-namespace:MinecraftLaunch.Classes.Models.Install;assembly=MinecraftLaunch"
    xmlns:properties="clr-namespace:YMCL.Main.Public.Langs"
    xmlns:ui="clr-namespace:FluentAvalonia.UI.Controls;assembly=FluentAvalonia"
    d:DesignHeight="800"
    d:DesignWidth="800"
    FontFamily="{DynamicResource Font}"
    FontWeight="{DynamicResource FontWeight}"
    mc:Ignorable="d">
    <Border
        x:Name="Root"
        ClipToBounds="True"
        CornerRadius="5">
        <Border.Transitions>
            <Transitions>
                <ThicknessTransition Property="Margin" Duration="0:0:0.45" />
            </Transitions>
        </Border.Transitions>
        <Border
            Margin="10,0"
            ClipToBounds="True"
            CornerRadius="5">
            <Grid>
                <Border
                    Margin="0,0,0,10"
                    ClipToBounds="True"
                    CornerRadius="5">
                    <ScrollViewer Name="InstallableVersionListRoot">
                        <StackPanel Spacing="10">
                            <Border
                                Name="BedrockRoot"
                                Padding="10"
                                VerticalAlignment="Top"
                                Background="{DynamicResource 1x}"
                                CornerRadius="5"
                                DockPanel.Dock="Top"
                                IsVisible="False"
                                Opacity="{DynamicResource Opacity}">
                                <DockPanel HorizontalAlignment="Stretch">
                                    <Image
                                        Width="15"
                                        Height="13"
                                        Margin="0,0,5,0"
                                        HorizontalAlignment="Right"
                                        DockPanel.Dock="Right">
                                        <Image.Source>
                                            <DrawingImage>
                                                <DrawingImage.Drawing>
                                                    <GeometryDrawing Brush="{DynamicResource TextColor}">
                                                        <GeometryDrawing.Geometry>
                                                            <PathGeometry Figures="M384,170.5C395.6669921875,170.5,405.6669921875,174.66700744628906,414,183L713,482C721.3330078125,490.3330078125 725.5,500.3330078125 725.5,512 725.5,523.6669921875 721.3330078125,533.6669921875 713,542L414,841C405.6669921875,849.3330078125 395.6669921875,853.5 384,853.5 372,853.5 361.9169921875,849.25 353.75,840.75 345.5830078125,832.25 341.5,822.1669921875 341.5,810.5 341.5,798.8330078125 345.6669921875,788.8330078125 354,780.5L622.5,512 354,243.5C345.6669921875,235.16700744628906 341.5,225.16700744628906 341.5,213.5 341.5,201.83299255371094 345.5830078125,191.75 353.75,183.25 361.9169921875,174.75 372,170.5 384,170.5z" />
                                                        </GeometryDrawing.Geometry>
                                                    </GeometryDrawing>
                                                </DrawingImage.Drawing>
                                            </DrawingImage>
                                        </Image.Source>
                                    </Image>
                                    <TextBlock
                                        Margin="2"
                                        FontFamily="{DynamicResource Font}"
                                        Text="{x:Static properties:MainLang.BedRockVersion}" />
                                </DockPanel>
                            </Border>
                            <ui:InfoBar
                                Name="LoadInstallableVersionListErrorInfoBar"
                                DockPanel.Dock="Top"
                                FontWeight="Light"
                                IsClosable="False"
                                IsOpen="True"
                                IsVisible="False"
                                Message="{x:Static properties:MainLang.GetInstallableVersionFail}"
                                Severity="Error">
                                <ui:InfoBar.ActionButton>
                                    <Button
                                        Name="ReloadLoadnstallableVersionListBtn"
                                        HorizontalAlignment="Right"
                                        Content="{x:Static properties:MainLang.Reload}" />
                                </ui:InfoBar.ActionButton>
                            </ui:InfoBar>
                            <Border
                                VerticalAlignment="Top"
                                Background="{DynamicResource 1x}"
                                ClipToBounds="True"
                                CornerRadius="5"
                                DockPanel.Dock="Top"
                                Opacity="{DynamicResource Opacity}">
                                <DockPanel>
                                    <TextBlock
                                        Margin="10,10,0,0"
                                        DockPanel.Dock="Top"
                                        FontFamily="{DynamicResource Font}"
                                        FontSize="13"
                                        Text="{x:Static properties:MainLang.LatestVersion}" />
                                    <StackPanel
                                        Margin="10,5,10,10"
                                        DockPanel.Dock="Bottom"
                                        Spacing="10">
                                        <Border
                                            Name="LatestReleaseVersionRoot"
                                            Padding="5"
                                            Background="{DynamicResource 2x}"
                                            CornerRadius="5">
                                            <DockPanel>
                                                <Image
                                                    Width="15"
                                                    Height="13"
                                                    Margin="0,0,10,0"
                                                    HorizontalAlignment="Right"
                                                    DockPanel.Dock="Right">
                                                    <Image.Source>
                                                        <DrawingImage>
                                                            <DrawingImage.Drawing>
                                                                <GeometryDrawing Brush="{DynamicResource TextColor}">
                                                                    <GeometryDrawing.Geometry>
                                                                        <PathGeometry Figures="M384,170.5C395.6669921875,170.5,405.6669921875,174.66700744628906,414,183L713,482C721.3330078125,490.3330078125 725.5,500.3330078125 725.5,512 725.5,523.6669921875 721.3330078125,533.6669921875 713,542L414,841C405.6669921875,849.3330078125 395.6669921875,853.5 384,853.5 372,853.5 361.9169921875,849.25 353.75,840.75 345.5830078125,832.25 341.5,822.1669921875 341.5,810.5 341.5,798.8330078125 345.6669921875,788.8330078125 354,780.5L622.5,512 354,243.5C345.6669921875,235.16700744628906 341.5,225.16700744628906 341.5,213.5 341.5,201.83299255371094 345.5830078125,191.75 353.75,183.25 361.9169921875,174.75 372,170.5 384,170.5z" />
                                                                    </GeometryDrawing.Geometry>
                                                                </GeometryDrawing>
                                                            </DrawingImage.Drawing>
                                                        </DrawingImage>
                                                    </Image.Source>
                                                </Image>
                                                <StackPanel Margin="2,0,0,0" DockPanel.Dock="Left">
                                                    <TextBlock>
                                                        <Run
                                                            Name="LatestReleaseVersionId"
                                                            FontSize="15"
                                                            Text="{x:Static properties:MainLang.Loading}" />
                                                        <Run
                                                            FontSize="12"
                                                            Foreground="{DynamicResource GrayColor}"
                                                            Text="{x:Static properties:MainLang.ReleaseVersion}" />
                                                    </TextBlock>
                                                    <TextBlock
                                                        Name="LatestReleaseVersionTime"
                                                        Margin="1,0,0,0"
                                                        FontSize="12"
                                                        Foreground="{DynamicResource GrayColor}"
                                                        Text="{x:Static properties:MainLang.Loading}" />
                                                </StackPanel>
                                            </DockPanel>
                                        </Border>
                                        <Border
                                            Name="LatestPreviewVersionRoot"
                                            Padding="5"
                                            Background="{DynamicResource 2x}"
                                            CornerRadius="5">
                                            <DockPanel>
                                                <Image
                                                    Width="15"
                                                    Height="12"
                                                    Margin="0,0,10,0"
                                                    HorizontalAlignment="Right"
                                                    DockPanel.Dock="Right">
                                                    <Image.Source>
                                                        <DrawingImage>
                                                            <DrawingImage.Drawing>
                                                                <GeometryDrawing Brush="{DynamicResource TextColor}">
                                                                    <GeometryDrawing.Geometry>
                                                                        <PathGeometry Figures="M384,170.5C395.6669921875,170.5,405.6669921875,174.66700744628906,414,183L713,482C721.3330078125,490.3330078125 725.5,500.3330078125 725.5,512 725.5,523.6669921875 721.3330078125,533.6669921875 713,542L414,841C405.6669921875,849.3330078125 395.6669921875,853.5 384,853.5 372,853.5 361.9169921875,849.25 353.75,840.75 345.5830078125,832.25 341.5,822.1669921875 341.5,810.5 341.5,798.8330078125 345.6669921875,788.8330078125 354,780.5L622.5,512 354,243.5C345.6669921875,235.16700744628906 341.5,225.16700744628906 341.5,213.5 341.5,201.83299255371094 345.5830078125,191.75 353.75,183.25 361.9169921875,174.75 372,170.5 384,170.5z" />
                                                                    </GeometryDrawing.Geometry>
                                                                </GeometryDrawing>
                                                            </DrawingImage.Drawing>
                                                        </DrawingImage>
                                                    </Image.Source>
                                                </Image>
                                                <StackPanel Margin="2,0,0,0" DockPanel.Dock="Left">
                                                    <TextBlock>
                                                        <Run
                                                            Name="LatestPreviewVersionId"
                                                            FontSize="15"
                                                            Text="{x:Static properties:MainLang.Loading}" />
                                                        <Run
                                                            FontSize="12"
                                                            Foreground="{DynamicResource GrayColor}"
                                                            Text="{x:Static properties:MainLang.PreviewVersion}" />
                                                    </TextBlock>
                                                    <TextBlock
                                                        Name="LatestPreviewVersionTime"
                                                        Margin="1,0,0,0"
                                                        FontSize="12"
                                                        Foreground="{DynamicResource GrayColor}"
                                                        Text="{x:Static properties:MainLang.Loading}" />
                                                </StackPanel>
                                            </DockPanel>
                                        </Border>
                                    </StackPanel>
                                </DockPanel>
                            </Border>
                            <Border
                                VerticalAlignment="Top"
                                Background="{DynamicResource 1x}"
                                CornerRadius="5"
                                DockPanel.Dock="Top"
                                Opacity="{DynamicResource Opacity}">
                                <Expander Header="{x:Static properties:MainLang.AllVersion}">
                                    <Grid Height="300">
                                        <ui:ProgressRing
                                            Name="AllVersionLoadingRing"
                                            Width="50"
                                            Height="50"
                                            Foreground="{DynamicResource SystemAccentColor}" />
                                        <DockPanel>
                                            <TextBox
                                                Name="AllVersionSearchBox"
                                                DockPanel.Dock="Top"
                                                Watermark="{x:Static properties:MainLang.SearchKeyword}" />
                                            <ListBox
                                                Name="AllVersionListView"
                                                Margin="0,10,0,0"
                                                DockPanel.Dock="Bottom"
                                                FontFamily="{DynamicResource Font}">
                                                <ListBox.ItemTemplate>
                                                    <DataTemplate DataType="ml:VersionManifestEntry">
                                                        <Border Margin="0,0,0,0" BorderThickness="0,4,0,4">
                                                            <DockPanel>
                                                                <Grid>
                                                                    <TextBlock
                                                                        FontFamily="{DynamicResource Font}"
                                                                        FontSize="14"
                                                                        Text="{Binding Id}" />
                                                                    <StackPanel Margin="0,17,0,0" Orientation="Horizontal">
                                                                        <TextBlock
                                                                            Margin="0,0,0,0"
                                                                            FontFamily="{DynamicResource Font}"
                                                                            FontSize="12"
                                                                            Foreground="{DynamicResource GrayColor}"
                                                                            Text="{Binding ReleaseTime}" />
                                                                    </StackPanel>
                                                                </Grid>
                                                            </DockPanel>
                                                        </Border>
                                                    </DataTemplate>
                                                </ListBox.ItemTemplate>
                                            </ListBox>
                                        </DockPanel>
                                    </Grid>
                                </Expander>
                            </Border>
                            <Border
                                VerticalAlignment="Top"
                                Background="{DynamicResource 1x}"
                                CornerRadius="5"
                                DockPanel.Dock="Top"
                                Opacity="{DynamicResource Opacity}">
                                <Expander Header="{x:Static properties:MainLang.ReleaseVersion}">
                                    <Grid Height="300">
                                        <ui:ProgressRing
                                            Name="ReleaseVersionLoadingRing"
                                            Width="50"
                                            Height="50"
                                            Foreground="{DynamicResource SystemAccentColor}" />
                                        <ListBox Name="ReleaseVersionListView" FontFamily="{DynamicResource Font}">
                                            <ListBox.ItemTemplate>
                                                <DataTemplate DataType="ml:VersionManifestEntry">
                                                    <Border Margin="0,0,0,0" BorderThickness="0,4,0,4">
                                                        <DockPanel>
                                                            <Grid>
                                                                <TextBlock
                                                                    FontFamily="{DynamicResource Font}"
                                                                    FontSize="14"
                                                                    Text="{Binding Id}" />
                                                                <StackPanel Margin="0,17,0,0" Orientation="Horizontal">
                                                                    <TextBlock
                                                                        Margin="0,0,0,0"
                                                                        FontFamily="{DynamicResource Font}"
                                                                        FontSize="12"
                                                                        Foreground="{DynamicResource GrayColor}"
                                                                        Text="{Binding ReleaseTime}" />
                                                                </StackPanel>
                                                            </Grid>
                                                        </DockPanel>
                                                    </Border>
                                                </DataTemplate>
                                            </ListBox.ItemTemplate>
                                        </ListBox>
                                    </Grid>
                                </Expander>
                            </Border>
                            <Border
                                VerticalAlignment="Top"
                                Background="{DynamicResource 1x}"
                                CornerRadius="5"
                                DockPanel.Dock="Top"
                                Opacity="{DynamicResource Opacity}">
                                <Expander Header="{x:Static properties:MainLang.PreviewVersion}">
                                    <Grid Height="300">
                                        <ui:ProgressRing
                                            Name="PreviewVersionLoadingRing"
                                            Width="50"
                                            Height="50"
                                            Foreground="{DynamicResource SystemAccentColor}" />
                                        <ListBox Name="PreviewVersionListView" FontFamily="{DynamicResource Font}">
                                            <ListBox.ItemTemplate>
                                                <DataTemplate DataType="ml:VersionManifestEntry">
                                                    <Border Margin="0,0,0,0" BorderThickness="0,4,0,4">
                                                        <DockPanel>
                                                            <Grid>
                                                                <TextBlock
                                                                    FontFamily="{DynamicResource Font}"
                                                                    FontSize="14"
                                                                    Text="{Binding Id}" />
                                                                <StackPanel Margin="0,17,0,0" Orientation="Horizontal">
                                                                    <TextBlock
                                                                        Margin="0,0,0,0"
                                                                        FontFamily="{DynamicResource Font}"
                                                                        FontSize="12"
                                                                        Foreground="{DynamicResource GrayColor}"
                                                                        Text="{Binding ReleaseTime}" />
                                                                </StackPanel>
                                                            </Grid>
                                                        </DockPanel>
                                                    </Border>
                                                </DataTemplate>
                                            </ListBox.ItemTemplate>
                                        </ListBox>
                                    </Grid>
                                </Expander>
                            </Border>
                            <Border
                                VerticalAlignment="Top"
                                Background="{DynamicResource 1x}"
                                CornerRadius="5"
                                DockPanel.Dock="Top"
                                Opacity="{DynamicResource Opacity}">
                                <Expander Header="{x:Static properties:MainLang.OldVersion}">
                                    <Grid Height="300">
                                        <ui:ProgressRing
                                            Name="OldVersionLoadingRing"
                                            Width="50"
                                            Height="50"
                                            Foreground="{DynamicResource SystemAccentColor}" />
                                        <ListBox Name="OldVersionListView" FontFamily="{DynamicResource Font}">
                                            <ListBox.ItemTemplate>
                                                <DataTemplate DataType="ml:VersionManifestEntry">
                                                    <Border Margin="0,0,0,0" BorderThickness="0,4,0,4">
                                                        <DockPanel>
                                                            <Grid>
                                                                <TextBlock
                                                                    FontFamily="{DynamicResource Font}"
                                                                    FontSize="14"
                                                                    Text="{Binding Id}" />
                                                                <StackPanel Margin="0,17,0,0" Orientation="Horizontal">
                                                                    <TextBlock
                                                                        Margin="0,0,0,0"
                                                                        FontFamily="{DynamicResource Font}"
                                                                        FontSize="12"
                                                                        Text="{Binding ReleaseTime}" />
                                                                </StackPanel>
                                                            </Grid>
                                                        </DockPanel>
                                                    </Border>
                                                </DataTemplate>
                                            </ListBox.ItemTemplate>
                                        </ListBox>
                                    </Grid>
                                </Expander>
                            </Border>
                        </StackPanel>
                    </ScrollViewer>
                </Border>
                <Border
                    Margin="0,0,0,10"
                    ClipToBounds="True"
                    CornerRadius="5">
                    <ScrollViewer Name="InstallPreviewRoot" IsVisible="False">
                        <DockPanel>
                            <Border
                                VerticalAlignment="Stretch"
                                Background="{DynamicResource 1x}"
                                CornerRadius="5"
                                Opacity="{DynamicResource Opacity}">
                                <StackPanel Margin="10" Spacing="10">
                                    <Border
                                        Padding="10"
                                        VerticalAlignment="Top"
                                        Background="{DynamicResource 2x}"
                                        CornerRadius="5">
                                        <DockPanel>
                                            <TextBlock DockPanel.Dock="Top" Text="{x:Static properties:MainLang.InstallPreview}" />
                                            <DockPanel DockPanel.Dock="Top">
                                                <DockPanel Margin="0,0,0,5" DockPanel.Dock="Left">
                                                    <TextBlock
                                                        Name="InstallPreviewIdText"
                                                        Margin="2,0,0,0"
                                                        DockPanel.Dock="Top"
                                                        FontSize="15"
                                                        Text="1.12.2" />
                                                    <TextBlock
                                                        Name="InstallPreviewAdditionalInstallText"
                                                        Margin="2,0,0,0"
                                                        DockPanel.Dock="Top"
                                                        FontSize="12"
                                                        Foreground="{DynamicResource GrayColor}"
                                                        Text="{x:Static properties:MainLang.NoAdditionalInstall}" />
                                                </DockPanel>
                                                <DockPanel
                                                    Margin="0,0,0,5"
                                                    HorizontalAlignment="Right"
                                                    VerticalAlignment="Bottom"
                                                    DockPanel.Dock="Right">
                                                    <Button Name="ViewUpdatedContentBtn" Content="{x:Static properties:MainLang.ViewUpdatedContent}" />
                                                    <Button
                                                        Name="BeginInstallBtn"
                                                        Margin="5,0,0,0"
                                                        Content="{x:Static properties:MainLang.BeginInstall}" />
                                                </DockPanel>
                                            </DockPanel>
                                            <TextBlock
                                                Name="CustomIdWarning"
                                                Margin="1,5,0,0"
                                                DockPanel.Dock="Bottom"
                                                FontSize="12"
                                                Foreground="Red">
                                                <Run Text="{x:Static properties:MainLang.TheVersionNameWillBeRenamedTo}" />
                                                <Run Name="CustomIdWarningText" />
                                            </TextBlock>
                                            <TextBox Name="InstallPreviewIdTextBox" DockPanel.Dock="Bottom" />
                                        </DockPanel>
                                    </Border>
                                    <Border
                                        Name="RetuenToListRoot"
                                        Padding="10"
                                        VerticalAlignment="Top"
                                        Background="{DynamicResource 2x}"
                                        CornerRadius="5">
                                        <DockPanel>
                                            <Image
                                                Width="15"
                                                Height="12"
                                                Margin="0,0,5,0"
                                                HorizontalAlignment="Right"
                                                DockPanel.Dock="Right">
                                                <Image.Source>
                                                    <DrawingImage>
                                                        <DrawingImage.Drawing>
                                                            <GeometryDrawing Brush="{DynamicResource TextColor}">
                                                                <GeometryDrawing.Geometry>
                                                                    <PathGeometry Figures="M384,170.5C395.6669921875,170.5,405.6669921875,174.66700744628906,414,183L713,482C721.3330078125,490.3330078125 725.5,500.3330078125 725.5,512 725.5,523.6669921875 721.3330078125,533.6669921875 713,542L414,841C405.6669921875,849.3330078125 395.6669921875,853.5 384,853.5 372,853.5 361.9169921875,849.25 353.75,840.75 345.5830078125,832.25 341.5,822.1669921875 341.5,810.5 341.5,798.8330078125 345.6669921875,788.8330078125 354,780.5L622.5,512 354,243.5C345.6669921875,235.16700744628906 341.5,225.16700744628906 341.5,213.5 341.5,201.83299255371094 345.5830078125,191.75 353.75,183.25 361.9169921875,174.75 372,170.5 384,170.5z" />
                                                                </GeometryDrawing.Geometry>
                                                            </GeometryDrawing>
                                                        </DrawingImage.Drawing>
                                                    </DrawingImage>
                                                </Image.Source>
                                            </Image>
                                            <TextBlock
                                                Name="MinecraftPreviewVerionId"
                                                Margin="2,0,0,0"
                                                DockPanel.Dock="Right"
                                                Text="1.12.2" />
                                            <TextBlock
                                                Margin="2,0,0,0"
                                                DockPanel.Dock="Right"
                                                Text="Minecraft" />
                                        </DockPanel>
                                    </Border>
                                    <Border
                                        Background="{DynamicResource 1x}"
                                        CornerRadius="5"
                                        Opacity="{DynamicResource Opacity}">
                                        <Expander>
                                            <Expander.Header>
                                                <TextBlock>
                                                    <Run Text="OptiFine" />
                                                    <Run Text=" - " />
                                                    <Run Name="SelectedOptiFine" Text="Null" />
                                                </TextBlock>
                                            </Expander.Header>
                                            <Grid>
                                                <ListBox
                                                    Name="OptiFineListView"
                                                    Height="250"
                                                    Tag="optifine">
                                                    <ListBox.ItemTemplate>
                                                        <DataTemplate DataType="ml:OptiFineInstallEntity">
                                                            <TextBlock>
                                                                <Run Text="{Binding Type}" />
                                                                <Run Text="{Binding Patch}" />
                                                            </TextBlock>
                                                        </DataTemplate>
                                                    </ListBox.ItemTemplate>
                                                </ListBox>
                                                <TextBlock
                                                    Name="NoOptifine"
                                                    Foreground="{DynamicResource GrayColor}"
                                                    Text="{x:Static properties:MainLang.NoAvailableItem}" />
                                                <ui:ProgressRing
                                                    Name="OptiFineLoading"
                                                    Width="50"
                                                    Height="50" />
                                            </Grid>
                                        </Expander>
                                    </Border>
                                    <Border
                                        Background="{DynamicResource 1x}"
                                        CornerRadius="5"
                                        Opacity="{DynamicResource Opacity}">
                                        <Expander>
                                            <Expander.Header>
                                                <TextBlock>
                                                    <Run Text="Forge" />
                                                    <Run Text=" - " />
                                                    <Run Name="SelectedForge" Text="Null" />
                                                </TextBlock>
                                            </Expander.Header>
                                            <Grid>
                                                <ListBox
                                                    Name="ForgeListView"
                                                    Height="250"
                                                    Tag="forge">
                                                    <ListBox.ItemTemplate>
                                                        <DataTemplate DataType="ml:ForgeInstallEntry">
                                                            <TextBlock Text="{Binding ForgeVersion}" />
                                                        </DataTemplate>
                                                    </ListBox.ItemTemplate>
                                                </ListBox>
                                                <TextBlock
                                                    Name="NoForge"
                                                    Foreground="{DynamicResource GrayColor}"
                                                    Text="{x:Static properties:MainLang.NoAvailableItem}" />
                                                <ui:ProgressRing
                                                    Name="ForgeLoading"
                                                    Width="50"
                                                    Height="50" />
                                            </Grid>
                                        </Expander>
                                    </Border>
                                    <Border
                                        Background="{DynamicResource 1x}"
                                        CornerRadius="5"
                                        Opacity="{DynamicResource Opacity}">
                                        <Expander>
                                            <Expander.Header>
                                                <TextBlock>
                                                    <Run Text="Fabric" />
                                                    <Run Text=" - " />
                                                    <Run Name="SelectedFabric" Text="Null" />
                                                </TextBlock>
                                            </Expander.Header>
                                            <Grid>
                                                <ListBox
                                                    Name="FabricListView"
                                                    Height="250"
                                                    Tag="fabric">
                                                    <ListBox.ItemTemplate>
                                                        <DataTemplate DataType="ml:FabricBuildEntry">
                                                            <TextBlock Text="{Binding BuildVersion}" />
                                                        </DataTemplate>
                                                    </ListBox.ItemTemplate>
                                                </ListBox>
                                                <TextBlock
                                                    Name="NoFabric"
                                                    Foreground="{DynamicResource GrayColor}"
                                                    Text="{x:Static properties:MainLang.NoAvailableItem}" />
                                                <ui:ProgressRing
                                                    Name="FabricLoading"
                                                    Width="50"
                                                    Height="50" />
                                            </Grid>
                                        </Expander>
                                    </Border>
                                    <Border
                                        Background="{DynamicResource 1x}"
                                        CornerRadius="5"
                                        Opacity="{DynamicResource Opacity}">
                                        <Expander>
                                            <Expander.Header>
                                                <TextBlock>
                                                    <Run Text="Quilt" />
                                                    <Run Text=" - " />
                                                    <Run Name="SelectedQuilt" Text="Null" />
                                                </TextBlock>
                                            </Expander.Header>
                                            <Grid>
                                                <ListBox
                                                    Name="QuiltListView"
                                                    Height="250"
                                                    Tag="quilt">
                                                    <ListBox.ItemTemplate>
                                                        <DataTemplate DataType="ml:QuiltBuildEntry">
                                                            <TextBlock Text="{Binding BuildVersion}" />
                                                        </DataTemplate>
                                                    </ListBox.ItemTemplate>
                                                </ListBox>
                                                <TextBlock
                                                    Name="NoQuilt"
                                                    Foreground="{DynamicResource GrayColor}"
                                                    Text="{x:Static properties:MainLang.NoAvailableItem}" />
                                                <ui:ProgressRing
                                                    Name="QuiltLoading"
                                                    Width="50"
                                                    Height="50" />
                                            </Grid>
                                        </Expander>
                                    </Border>
                                </StackPanel>
                            </Border>
                        </DockPanel>
                    </ScrollViewer>
                </Border>
            </Grid>
        </Border>
    </Border>
</UserControl>