                        <StackPanel Orientation="Horizontal" Width="100">
                            <TextBlock
                                FontFamily="{DynamicResource Font}"
                                FontSize="16"
                                Name="TotalTimeText"
                                Text="{Binding UiProperty.MusicTotalTime, Converter={StaticResource MsToTimeConverter}}" />
                            <TextBlock
                                FontFamily="{DynamicResource Font}"
                                FontSize="16"
                                Text="/" />
                            <TextBlock
                                FontFamily="{DynamicResource Font}"
                                FontSize="16"
                                Name="CurrentTimeText"
                                Text="{Binding UiProperty.MusicCurrentTime, Converter={StaticResource MsToTimeConverter}}" />
                        </StackPanel>
                    </DockPanel>
                </Grid>
            </Border>

            <Grid DockPanel.Dock="Top" Margin="10,10,10,10">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition MinWidth="200" Width="*" />
                    <ColumnDefinition Width="10" />
                    <ColumnDefinition MinWidth="300" Width="300" />
                </Grid.ColumnDefinitions>
                <Border
                    ClipToBounds="True"
                    CornerRadius="{DynamicResource MainCornerRadius}"
                    Grid.Column="0">
                    <ContentControl Name="LeftControl" />
                </Border>
                <GridSplitter
                    Background="Transparent"
                    Grid.Column="1"
                    ResizeDirection="Columns" />
                <Border
                    ClipToBounds="True"
                    CornerRadius="{DynamicResource MainCornerRadius}"
                    Grid.Column="2">
                    <ContentControl Name="RightControl" />
                </Border>
            </Grid>
        </DockPanel>
        <Border
            Background="{DynamicResource 2x}"
            ClipToBounds="True"
            CornerRadius="{DynamicResource MainCornerRadius}"
            Height="30"
            HorizontalAlignment="Right"
            Margin="0,0,70,80"
            Name="VolumeRoot"
            Opacity="0"
            Padding="-5"
            VerticalAlignment="Bottom">
            <Border.Transitions>
                <Transitions>
                    <DoubleTransition Duration="0:0:0.15" Property="Opacity" />
                </Transitions>
            </Border.Transitions>
            <DockPanel VerticalAlignment="Center">
                <StackPanel
                    DockPanel.Dock="Right"
                    Margin="-5,0,15,0"
                    Orientation="Horizontal"
                    VerticalAlignment="Center">
                    <TextBlock
                        Name="VolumeText"
                        Text="{Binding SettingEntry.Volume, Converter={StaticResource DoubleToIntByRoundConverter}}"
                        TextAlignment="Center"
                        Width="25" />
                    <TextBlock Text="%" />
                </StackPanel>
                <Slider
                    Margin="15,0"
                    Maximum="100"
                    Name="VolumeSlider"
                    Value="{Binding SettingEntry.Volume}"
                    Width="100" />
            </DockPanel>
        </Border>
    </Grid>
</UserControl>