<UserControl
    d:DesignHeight="450"
    d:DesignWidth="800"
    mc:Ignorable="d"
    x:Class="YMCL.Main.Views.Main.Pages.Launch.LaunchPage"
    xmlns="https://github.com/avaloniaui"
    xmlns:cl="clr-namespace:YMCL.Main.Public.Classes;assembly=YMCL.Main"
    xmlns:class="clr-namespace:YMCL.Main.Public.Classes;assembly=YMCL.Main"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:ml="clr-namespace:MinecraftLaunch.Classes.Models.Game;assembly=MinecraftLaunch"
    xmlns:properties="clr-namespace:YMCL.Main.Public.Langs"
    xmlns:uc="clr-namespace:YMCL.Main.Public.Controls"
    xmlns:ui="clr-namespace:FluentAvalonia.UI.Controls;assembly=FluentAvalonia"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Border x:Name="Root">
        <Border.Transitions>
            <Transitions>
                <ThicknessTransition Duration="0:0:0.45" Property="Margin" />
            </Transitions>
        </Border.Transitions>
        <Grid>
            <DockPanel>
                <Border
                    Background="{DynamicResource 1x}"
                    ClipToBounds="True"
                    CornerRadius="5"
                    DockPanel.Dock="Right"
                    HorizontalAlignment="Right"
                    Margin="10"
                    MaxWidth="280"
                    MinWidth="240"
                    Padding="10">
                    <DockPanel>
                        <DockPanel
                            DockPanel.Dock="Bottom"
                            HorizontalAlignment="Stretch"
                            VerticalAlignment="Bottom">
                            <ComboBox
                                DockPanel.Dock="Top"
                                FontFamily="{DynamicResource Font}"
                                HorizontalAlignment="Stretch"
                                Margin="0,0,0,5"
                                Name="AccountComboBox">
                                <ComboBox.ItemTemplate>
                                    <DataTemplate DataType="class:AccountInfo">
                                        <TextBlock>
                                            <Run Text="[" />
                                            <Run Text="{Binding AccountType}" />
                                            <Run Text="]" />
                                            <Run Text="{Binding Name}" />
                                        </TextBlock>
                                    </DataTemplate>
                                </ComboBox.ItemTemplate>
                            </ComboBox>
                            <DockPanel
                                DockPanel.Dock="Top"
                                HorizontalAlignment="Stretch"
                                VerticalAlignment="Bottom">
                                <Button
                                    DockPanel.Dock="Top"
                                    HorizontalContentAlignment="Stretch"
                                    Margin="0,0,0,5"
                                    Name="LaunchBtn"
                                    Width="{Binding $parent.Bounds.Width}">
                                    <StackPanel>
                                        <TextBlock
                                            FontFamily="{DynamicResource Font}"
                                            FontSize="14"
                                            HorizontalAlignment="Center"
                                            Margin="0,3,0,0"
                                            Text="{x:Static properties:MainLang.LaunchGame}"
                                            VerticalAlignment="Center" />
                                        <TextBlock
                                            FontFamily="{DynamicResource Font}"
                                            FontSize="12"
                                            HorizontalAlignment="Center"
                                            Margin="0,2,0,3"
                                            Text="{x:Static properties:MainLang.NoChooseGame}"
                                            TextAlignment="Center"
                                            TextWrapping="Wrap"
                                            VerticalAlignment="Center"
                                            x:Name="GameCoreText" />
                                    </StackPanel>
                                </Button>
                                <UniformGrid
                                    Columns="2"
                                    DockPanel.Dock="Bottom"
                                    Height="34"
                                    Rows="1">
                                    <Button
                                        Content="{x:Static properties:MainLang.VersionList}"
                                        FontFamily="{DynamicResource Font}"
                                        HorizontalAlignment="Stretch"
                                        Margin="0,0,3.25,0"
                                        Name="VersionListBtn"
                                        VerticalAlignment="Stretch" />
                                    <Button
                                        Content="{x:Static properties:MainLang.VersionSetting}"
                                        FontFamily="{DynamicResource Font}"
                                        HorizontalAlignment="Stretch"
                                        Margin="3.25,0,0,0"
                                        Name="VersionSettingBtn"
                                        VerticalAlignment="Stretch" />
                                </UniformGrid>
                            </DockPanel>
                        </DockPanel>
                        <Border
                            Background="{DynamicResource 2x}"
                            ClipToBounds="True"
                            CornerRadius="5"
                            DockPanel.Dock="Top"
                            Height="90"
                            HorizontalAlignment="Center"
                            VerticalAlignment="Center"
                            Width="90">
                            <Image Name="Head" />
                        </Border>
                    </DockPanel>
                </Border>
                <Border
                    ClipToBounds="True"
                    CornerRadius="5"
                    Margin="10,10,0,10"
                    Name="CustomPageRoot" />
            </DockPanel>
            <Border
                Background="{DynamicResource 1x}"
                ClipToBounds="True"
                CornerRadius="5"
                IsVisible="False"
                Margin="10,10,10,10"
                Name="VersionListRoot"
                Padding="10">
                <Border.Transitions>
                    <Transitions>
                        <ThicknessTransition Duration="0:0:0.20" Property="Margin" />
                    </Transitions>
                </Border.Transitions>
                <DockPanel>
                    <DockPanel DockPanel.Dock="Top" VerticalAlignment="Top">
                        <Button
                            Content="{x:Static properties:MainLang.ReturnWithIcon}"
                            FontFamily="{DynamicResource Font}"
                            Margin="0,0,5,0"
                            Name="CloseVersionListBtn" />
                        <Button
                            Content="{x:Static properties:MainLang.OpenFolder}"
                            FontFamily="{DynamicResource Font}"
                            Margin="0,0,5,0"
                            Name="OpenSelectedMinecraftFolderBtn" />
                        <Button
                            Content="{x:Static properties:MainLang.Refresh}"
                            FontFamily="{DynamicResource Font}"
                            Margin="0,0,5,0"
                            Name="RefreshVersionListBtn" />
                        <ComboBox
                            FontFamily="{DynamicResource Font}"
                            HorizontalAlignment="Stretch"
                            Name="MinecraftFolderComboBox" />
                    </DockPanel>
                    <ListBox
                        FontFamily="{DynamicResource Font}"
                        Margin="0,5,0,0"
                        Name="VersionListView">
                        <ListBox.ItemTemplate>
                            <DataTemplate DataType="ml:GameEntry">
                                <StackPanel Margin="0,3,0,3">
                                    <TextBlock FontFamily="{DynamicResource Font}" Text="{Binding Id}" />
                                    <TextBlock
                                        FontFamily="{DynamicResource Font}"
                                        FontSize="12"
                                        Foreground="{DynamicResource GrayColor}"
                                        Margin="0,-4,0,0">
                                        <Run Text="{Binding Type}" />
                                        <Run Text=" " />
                                        <Run Text="{Binding Version}" />
                                        <Run Text=" " />
                                        <Run Text="{Binding MainLoaderType}" />
                                        <Run Text=" " />
                                        <Run Text="Java" />
                                        <Run Text="{Binding JavaVersion}" />
                                        <Run Text=" " />
                                        <Run Text="{Binding MainClass}" />
                                    </TextBlock>
                                </StackPanel>
                            </DataTemplate>
                        </ListBox.ItemTemplate>
                    </ListBox>
                </DockPanel>
            </Border>
            <Border
                Background="{DynamicResource 1x}"
                BorderBrush="{DynamicResource 1x}"
                BorderThickness="2"
                ClipToBounds="True"
                CornerRadius="5"
                IsVisible="False"
                Margin="10,10,10,10"
                Name="VersionSettingRoot">
                <Border.Transitions>
                    <Transitions>
                        <ThicknessTransition Duration="0:0:0.20" Property="Margin" />
                    </Transitions>
                </Border.Transitions>
                <DockPanel>
                    <Border
                        Background="{DynamicResource 1x}"
                        ClipToBounds="True"
                        CornerRadius="5">
